{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },

    "preview-apk": {
      "extends": "preview",
      "android": { "buildType": "apk" }
    },
    "production-apk": {
      "extends": "production",
      "android": { "buildType": "apk" }
    }
  },
  "submit": {
    "production": {}
  }
}
